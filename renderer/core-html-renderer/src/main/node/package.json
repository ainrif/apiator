{
  "name": "resources",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run dev && ./node_modules/.bin/cypress run",
    "purge": "rm -rf ./dist && rm -rf ./node_modules",
    "dev": "./node_modules/.bin/parcel index.html",
    "posix-build": "rm -rf ./dist && ./node_modules/.bin/parcel build ./index.html && node posthtml.js",
    "posix-build-ci": "rm -rf ./dist && npm ci && ./node_modules/.bin/parcel build ./index.html && node posthtml.js",
    "size": "npm run posix-build && size-limit"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "size-limit": [
    {
      "webpack": false,
      "path": "dist/*.js",
      "limit": "1000 ms"
    }
  ],
  "dependencies": {
    "clipboard": "^1.5.5",
    "handlebars": "^4.1.0",
    "jquery": "^3.4.0",
    "lodash": "^4.17.13",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "cypress": "^3.4.0",
    "eslint": "^6.1.0",
    "font-awesome": "^4.5.0",
    "less": "^3.9.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-apiator": "file:parcel-plugin-apiator",
    "posthtml-inline-assets": "^3.0.0",
    "size-limit": "^1.3.7"
  }
}
