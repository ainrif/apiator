<div class="sidebar">
    <ul class="nav nav-sidebar complementary">
        <a href="#endpoints"> <i class="fa fa-plug"></i> <span class="endpoint">Endpoints</span></a>
        <li class="endpoints side-container">
            <ul class="nav nav-sidebar">
                {{#each apiContexts}}
                    <li>
                        <a class="gray" href="#{{apiPath}}"><span class="endpoint">{{apiPath}}</span></a>
                    </li>
                    {{#each apiEndpoints}}
                        <li>
                            <a title="{{path}}" data-href="{{urler  method=method apiPath=../apiPath path=path}}" href="{{urler  method=method apiPath=../apiPath path=path}}" data-target=""><span class="endpoint {{method}}">{{path}}</span></a>
                        </li>
                    {{/each}}
                {{/each}}
            </ul>
        </li>
        <a href="#types"> <i class="fa fa-file-text"></i> <span class="endpoint">Types</span></a>
        <li class="types side-container">
            <ul class="nav nav-sidebar">
                {{#each usedApiTypes}}
                    <li>
                        <a title="{{shrinkByDots type}}" href="#{{shrinkByDots type}}" data-target=""><span class="endpoint {{modelType}}">{{shrinkByDots type}}</span></a>
                    </li>
                {{/each}}
            </ul>
        </li>
        <a href="#enumerations"> <i class="fa fa-file-text"></i> <span class="endpoint">Enumerations</span></a>
        <li class="enumerations side-container">
            <ul class="nav nav-sidebar">
                {{#each usedEnumerations}}
                    <li>
                        <a title="{{shrinkByDots type}}" href="#{{shrinkByDots type}}" data-target=""><span class="endpoint {{modelType}}">{{shrinkByDots type}}</span></a>
                    </li>
                {{/each}}
            </ul>
        </li>
    </ul>
</div>
