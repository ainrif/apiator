<div class="main" role="main">
    {{#each apiContexts}}
        <div class="target-marker" id="{{apiPath}}"></div>
        <h4 class="page-header">{{apiPath}}</h4>
        {{#each apiEndpoints}}
            <div class="target-marker" id="_{{lower method}}_{{../apiPath}}{{path}}"></div>
            <div class="panel {{panelStyle method}}">
                <div class="panel-heading">
                    <span class="gray endpoint {{method}}">{{path}}</span>
                    <span class="span-left" title='Click to copy' data-clipboard-text="{{copyUrler  method=method apiPath=../apiPath path=path}}"><i class="fa fa-link"></i></span>

                    <div class="navbar-right link-to-java">
                        <span class="span-info">{{../name}}.{{name}}</span>
                        <span class="span-right"><i title="Click to copy" class="fa fa-clipboard" data-clipboard-text="{{../name}}#{{name}}"></i></span>
                    </div>
                </div>
                <div class="panel-body">
                    <div class="container-fluid">
                        {{#if params}}
                            <div class="row">
                                <h5>Parameters</h5>

                                <div class="col-xs-12 col-md-10">
                                    {{#each params}}
                                        <div class="row">
                                            <div class="col-xs-2 col-sm-2 col-md-1 parameter lovercase">
                                                ({{httpParamType}})
                                            </div>
                                            <div class="col-xs-2 col-sm-1 col-md-6 gap_x_r">
                                                <span class="type object">{{modelType}}</span>
                                                {{~#if name~}}
                                                    <span class="gray italic">
                                                                {{~name~}}
                                                        {{~#if defaultValue~}}
                                                            <span class="gray span-info">={{defaultValue}}</span>
                                                        {{~/if~}}
                                                            </span>
                                                {{~/if~}}
                                                {{~#if basedOn~}}
                                                    {{#each basedOn}}
                                                        {{#if this.type}}
                                                            <a href="#" class="object object-link">{{this.type}}</a>
                                                        {{else}}
                                                            <span class="lovercase">{{this.modelType}}</span>
                                                        {{/if}}
                                                    {{/each}}
                                                {{~/if~}}
                                                {{~#if type~}}
                                                    <span>→</span>
                                                    <span class="object-link"><a href="#{{shrinkByDots type}}">{{shrinkByDots type}}</a></span>
                                                {{~/if~}}
                                            </div>
                                        </div>
                                    {{/each}}
                                </div>
                            </div>
                        {{/if}}
                        <div class="row">
                            <h5>Responses</h5>

                            <div class="col-xs-12 col-md-10">
                                <div class="row">
                                    <div class="col-xs-1 col-md-1 response-code">200</div>
                                    <!--<div class="col-xs-4 col-md-3 response-format  object">application/json</div>-->
                                    <div class="col-xs-4 col-md-4 ">

                                        {{~#if returnType.type~}}
                                            <span class="name object-link">
                                                <a href="#{{shrinkByDots returnType.type}}">
                                                    {{~shrinkByDots returnType.type~}}
                                                </a>
                                            </span>
                                        {{else}}
                                            <span class="type">
                                                {{~returnType.modelType~}}
                                            </span>
                                        {{/if~}}
                                        {{~#if returnType.basedOn}}
                                            {{~#each returnType.basedOn}}
                                                {{{responseTyper this}}}
                                            {{/each~}}
                                        {{/if~}}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    {{/each}}

    <h4 class="page-header">Types</h4>

    <div class="target-marker" id="types"></div>
    {{#each usedApiTypes}}
        <div class="target-marker" id="{{shrinkByDots type}}"></div>
        <div class="panel panel-info">
            <div class="panel-heading">
                <span class="gray endpoint {{method}}">{{shrinkByDots type}}</span>
            </div>
            <div class="panel-body">
                <div class="container-fluid">
                    <div class="row">
                        <h5>Fields</h5>

                        <div class="col-xs-12 col-md-10">
                            {{#each fields}}
                                <div class="row">
                                    <div class="col-sm-4 typeNames">
                                        <span class="text-wrap" title="{{name}}">{{name}}</span>
                                    </div>
                                    <div class="col-sm-6 gap_x_r dictionary">
                                        <span class="type object">{{modelType}}</span>
                                        <!--{{#if name}}<span class="gray italic">{{name}}</span>{{/if}}-->
                                        {{~#if basedOn~}}
                                            {{#each basedOn}}
                                                {{#if this.type}}
                                                    <span>of </span>
                                                    <a href="#{{shrinkByDots this.type}}" class=" object-link">{{shrinkByDots this.type}}</a>
                                                {{else}}
                                                    <span class="lovercase">{{this.modelType}}</span>
                                                {{/if}}
                                            {{/each}}
                                        {{~/if~}}
                                        {{~#if type~}}
                                            <span>→</span>
                                            <span class="object-link"><a href="#{{shrinkByDots type}}">{{shrinkByDots type}}</a></span>
                                        {{~/if~}}
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
    <div class="target-marker" id="enumerations"></div>
    <h4 class="page-header">Enumerations</h4>
    {{#each usedEnumerations}}
        <div class="target-marker" id="{{shrinkByDots type}}"></div>
        <div class="panel panel-info">
            <div class="panel-heading">
                <span class="gray endpoint {{method}}">{{shrinkByDots type}}</span>
            </div>
            <div class="panel-body">
                <div class="container-fluid">
                    <div class="row">
                        <h5>Values</h5>

                        <div class="col-xs-12 col-md-10">
                            {{#each values}}
                                <div class="row">
                                    <div class="col-xs-2 col-sm-2 col-md-3">
                                        {{this}}
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
</div>
